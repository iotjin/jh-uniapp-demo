{
	"easycom": {
		// npm方式安装的配置
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		// 下载安装方式配置
		// "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue",
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarBackgroundColor": "#FFFFFF"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarBackgroundColor": "#FFFFFF"
				// "navigationStyle": "custom" // 使用自定义导航栏
				//导航条隐藏，没有占位
				// "transparentTitle": "always"
				// "app-plus": {"titleNView": false} 
			}
		},
		{
			"path": "pages/module1/index",
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/module2/index",
			"style": {
				"navigationBarTitleText": "Demo",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/module3/index",
			"style": {
				"navigationStyle": "custom", // 使用自定义导航栏
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}
		},
		{ "path": "pages/module3/set/set" },
		{ "path": "pages/module3/about/about" },
		{
			"path": "pages/module4/index",
			"style": {
				"navigationBarTitleText": "模块四",
				"enablePullDownRefresh": false
			}
		},
		{ "path": "pages/module4/jump/jump" },
		{ "path": "pages/demoList/demoList" },
		{ "path": "pages/demoList/eCharts/eChartsDemoList" },
		{ "path": "pages/demoList/eCharts/eCharts1" },
		{ "path": "pages/demoList/eCharts/eCharts2" },
		{ "path": "pages/demoList/eCharts/eCharts3" },
		{ "path": "pages/demoList/eCharts/eCharts4" },
		{ "path": "pages/demoList/eCharts/eCharts5" },
		{ "path": "pages/demoList/gridView/gridViewDemoList" },
		{ "path": "pages/demoList/gridView/gridView1" },
		{ "path": "pages/demoList/gridView/gridView2" },
		{ "path": "pages/demoList/gridView/gridView3" },
		{ "path": "pages/demoList/gridView/gridView4" },
		{ "path": "pages/demoList/listView/listViewDemoList" },
		{ "path": "pages/demoList/listView/listView1" },
		{
			"path": "pages/demoList/listView/listView2",
			"style": {
				"enablePullDownRefresh": true
			}
		},
		{ "path": "pages/demoList/listView/listView3" },
		{ "path": "pages/demoList/listView/listView4" },
		{ "path": "pages/demoList/listView/listView5" },
		{ "path": "pages/demoList/listView/listView6" },
		{ "path": "pages/demoList/listView/listView7" },
		{ "path": "pages/demoList/listView/listView8" },
		{ "path": "pages/demoList/map/mapDemoList" },
		{ "path": "pages/demoList/map/map/map4" },
		{ "path": "pages/demoList/ui/uiDemoList" },
		{ "path": "pages/demoList/ui/ui1/uForm1" },
		{ "path": "pages/demoList/other/otherDemoList" },
		{ "path": "pages/demoList/other/other/baseNavBarDemo" },
		{ "path": "pages/demoList/other/other/alertDemo" },
		{ "path": "pages/demoList/other/other/systemInfo" },
		{ "path": "pages/demoList/other/other/encryptDemo" }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#38BC9D",
		"backgroundColor": "#F8F8F8",
		"backgroundColorTop": "#FFFFFF", // iOS APP真机bounce回弹区域默认灰色,建议统一重置为白色
		"usingComponents": {
			//#ifdef  MP-WEIXIN
			"van-button": "/wxcomponents/vant/button/index",
			"van-cell": "/wxcomponents/vant/cell/index",
			"van-cell-group": "/wxcomponents/vant/cell-group/index"
			//#endif
		}
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#38BC9D",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/module1/index",
				"iconPath": "static/images/tab/ic_tab1.png",
				"selectedIconPath": "static/images/tab/ic_tab1_select.png",
				"text": "首页"
		}, {
				// "pagePath": "pages/module2/index",
				"pagePath": "pages/demoList/demoList",
				"iconPath": "static/images/tab/ic_tab2.png",
				"selectedIconPath": "static/images/tab/ic_tab2_select.png",
				"text": "Demo"
		}, {
				"pagePath": "pages/module3/index",
				"iconPath": "static/images/tab/ic_tab3.png",
				"selectedIconPath": "static/images/tab/ic_tab3_select.png",
				"text": "我的"
		}
		// , {
		// 	"pagePath": "pages/module4/index",
		// 	"iconPath": "static/images/tab/ic_tab3.png",
		// 	"selectedIconPath": "static/images/tab/ic_tab3_select.png",
		// 	"text": "模块4"
		// },
		]
	}

}
